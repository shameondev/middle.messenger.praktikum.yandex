{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off"
  }
}
